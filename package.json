{
  "name": "james-test",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "autoprefixer": "^9.5.1",
    "npm-run-all": "^4.1.5",
    "postcss": "^5.2.18",
    "postcss-cli": "^6.1.1",
    "postcss-flexbugs-fixes": "4.1.0",
    "postcss-import": "^12.0.1",
    "postcss-load-config": "^2.0.0",
    "postcss-loader": "3.0.0",
    "postcss-modules": "^1.4.1",
    "postcss-nested": "^4.1.1",
    "postcss-preset-env": "^6.5.0",
    "prop-types": "^15.7.2",
    "react": "^16.8.6",
    "react-dom": "^16.8.6",
    "react-scripts": "3.0.1",
    "underscore": "^1.9.1"
  },
  "scripts": {
    "build:css": "postcss src/styles/main.css -o src/index.css",
    "watch:css": "postcss src/styles/main.css -o src/index.css -w",
    "start": "npm-run-all -p watch:css start-js",
    "start-js": "react-scripts start",
    "build-js": "react-scripts build",
    "build": "npm-run-all build:css build-js",
    "test": "react-scripts test --env=jsdom --setupFiles ./src/setupTests.js --coverage",
    "eject": "react-scripts eject",
    "storybook": "start-storybook -p 9009 -s public",
    "build-storybook": "build-storybook -s public"
  },
  "eslintConfig": {
    "extends": "react-app"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@babel/core": "^7.4.4",
    "@babel/runtime": "^7.4.5",
    "@storybook/addon-actions": "^5.0.11",
    "@storybook/addon-links": "^5.0.11",
    "@storybook/addon-storyshots": "^5.1.3",
    "@storybook/addons": "^5.0.11",
    "@storybook/react": "^5.0.11",
    "enzyme": "^3.9.0",
    "enzyme-adapter-react-16": "^1.13.2",
    "react-test-renderer": "^16.8.6",
    "require-context.macro": "^1.0.4",
    "tailwindcss": "^1.0.3"
  }
}
